#!/bin/bash

PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin:~/bin
export PATH
NAME_BIN="rclone"
### BEGIN INIT INFO
# Provides:          rclone
# Required-Start:    $all
# Required-Stop:     $all
# Default-Start:     2 3 4 5
# Default-Stop:      0 1 6
# Short-Description: Start rclone at boot time
# Description:       Enable rclone by daemon.
### END INIT INFO

NAME="cloud17-zm" #Rclone配置时填写的name
REMOTE='JAV_output'  #远程文件夹，网盘里的挂载的一个文件夹，留空为整个网盘
LOCAL='/home/cloud/cloud17-zm'  #挂载地址，VPS本地挂载目录
LOG="/$HOME/.rclone/rcloned.log"

Green_font_prefix="\033[32m" && Red_font_prefix="\033[31m" && Green_background_prefix="\033[42;37m" && Red_background_prefix="\033[41;37m" && Font_color_suffix="\033[0m"
Info="${Green_font_prefix}[信息]${Font_color_suffix}"
Error="${Red_font_prefix}[错误]${Font_color_suffix}"
RETVAL=0

check_running(){
	PID="$(ps -C $NAME_BIN -o pid= |head -n1 |grep -o '[0-9]\{1,\}')"
	if [[ ! -z ${PID} ]]; then
		return 0
	else
		return 1
	fi
}
do_start(){
	check_running
	if [[ $? -eq 0 ]]; then
		echo -e "${Info} $NAME_BIN (PID ${PID}) 正在运行..." && exit 0
	else
		fusermount -zuq $LOCAL >/dev/null 2>&1
		mkdir -p $LOCAL
		mkdir -p ${LOG%/*}
		sudo /usr/bin/rclone mount -vv cloud1-zm:$REMOTE /home/cloud/cloud1-zm --copy-links --no-check-certificate --allow-other --allow-non-empty --use-mmap --umask 000 --buffer-size 1024M --transfers 3  --daemon --default-permissions --vfs-read-chunk-size 1024M --vfs-read-chunk-size-limit 2048M --vfs-cache-mode full --vfs-cache-max-age 24h --vfs-cache-max-size 10G --vfs-read-ahead 5G --attr-timeout 10m --multi-thread-streams 5 --multi-thread-cutoff 160M --multi-thread-write-buffer-size 512M --tpslimit 6 --tpslimit-burst 3 --user-agent "ISV|rclone.org|rclone/v1.55.1" --cache-dir /home/cache/cloud1-zm > "${LOG}" 2>&1 &
		sleep 5s
		sudo /usr/bin/rclone mount -vv cloud2-zm:$REMOTE /home/cloud/cloud2-zm --copy-links --no-check-certificate --allow-other --allow-non-empty --use-mmap --umask 000 --buffer-size 1024M --transfers 3  --daemon --default-permissions --vfs-read-chunk-size 1024M --vfs-read-chunk-size-limit 2048M --vfs-cache-mode full --vfs-cache-max-age 24h --vfs-cache-max-size 10G --vfs-read-ahead 5G --attr-timeout 10m --multi-thread-streams 5 --multi-thread-cutoff 160M --multi-thread-write-buffer-size 512M --tpslimit 6 --tpslimit-burst 3 --user-agent "ISV|rclone.org|rclone/v1.55.1" --cache-dir /home/cache/cloud2-zm > "${LOG}" 2>&1 &
		sleep 5s
		sudo /usr/bin/rclone mount -vv cloud3-zm:$REMOTE /home/cloud/cloud3-zm --copy-links --no-check-certificate --allow-other --allow-non-empty --use-mmap --umask 000 --buffer-size 1024M --transfers 3  --daemon --default-permissions --vfs-read-chunk-size 1024M --vfs-read-chunk-size-limit 2048M --vfs-cache-mode full --vfs-cache-max-age 24h --vfs-cache-max-size 10G --vfs-read-ahead 5G --attr-timeout 10m --multi-thread-streams 5 --multi-thread-cutoff 160M --multi-thread-write-buffer-size 512M --tpslimit 6 --tpslimit-burst 3 --user-agent "ISV|rclone.org|rclone/v1.55.1" --cache-dir /home/cache/cloud3-zm > "${LOG}" 2>&1 &
		sleep 5s
		sudo /usr/bin/rclone mount -vv cloud4-zm:$REMOTE /home/cloud/cloud4-zm --copy-links --no-check-certificate --allow-other --allow-non-empty --use-mmap --umask 000 --buffer-size 1024M --transfers 3  --daemon --default-permissions --vfs-read-chunk-size 1024M --vfs-read-chunk-size-limit 2048M --vfs-cache-mode full --vfs-cache-max-age 24h --vfs-cache-max-size 10G --vfs-read-ahead 5G --attr-timeout 10m --multi-thread-streams 5 --multi-thread-cutoff 160M --multi-thread-write-buffer-size 512M --tpslimit 6 --tpslimit-burst 3 --user-agent "ISV|rclone.org|rclone/v1.55.1" --cache-dir /home/cache/cloud4-zm > "${LOG}" 2>&1 &
		sleep 5s
		sudo /usr/bin/rclone mount -vv cloud5-zm:$REMOTE /home/cloud/cloud5-zm --copy-links --no-check-certificate --allow-other --allow-non-empty --use-mmap --umask 000 --buffer-size 1024M --transfers 3  --daemon --default-permissions --vfs-read-chunk-size 1024M --vfs-read-chunk-size-limit 2048M --vfs-cache-mode full --vfs-cache-max-age 24h --vfs-cache-max-size 10G --vfs-read-ahead 5G --attr-timeout 10m --multi-thread-streams 5 --multi-thread-cutoff 160M --multi-thread-write-buffer-size 512M --tpslimit 6 --tpslimit-burst 3 --user-agent "ISV|rclone.org|rclone/v1.55.1" --cache-dir /home/cache/cloud5-zm > "${LOG}" 2>&1 &
