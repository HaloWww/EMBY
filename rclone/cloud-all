		sleep 5s
		sudo /usr/bin/rclone mount -vv mix4-xg: /home/mix4-xg --copy-links --no-check-certificate --allow-other --allow-non-empty --use-mmap --umask 000 --buffer-size 1024M --transfers 3  --daemon --default-permissions --vfs-read-chunk-size 1024M --vfs-read-chunk-size-limit 2048M --vfs-cache-mode full --vfs-cache-max-age 24h --vfs-cache-max-size 10G --vfs-read-ahead 5G --attr-timeout 10m --multi-thread-streams 5 --multi-thread-cutoff 160M --multi-thread-write-buffer-size 512M --tpslimit 6 --tpslimit-burst 3 --user-agent "ISV|rclone.org|rclone/v1.55.1" --cache-dir /home/cache/mix4-xg > "${LOG}" 2>&1 &
		sleep 5s
		check_running
		if [[ $? -eq 0 ]]; then
			echo -e "${Info} $NAME_BIN 启动成功 !"
		else
			echo -e "${Error} $NAME_BIN 启动失败 !"
		fi
	fi
}
do_stop(){
	check_running
	if [[ $? -eq 0 ]]; then
		kill -9 ${PID}
		RETVAL=$?
		if [[ $RETVAL -eq 0 ]]; then
			echo -e "${Info} $NAME_BIN 停止成功 !"
		else
			echo -e "${Error} $NAME_BIN 停止失败 !"
		fi
	else
		echo -e "${Info} $NAME_BIN 未运行"
		RETVAL=1
	fi
	fusermount -zuq $LOCAL >/dev/null 2>&1
}
do_status(){
	check_running
	if [[ $? -eq 0 ]]; then
		echo -e "${Info} $NAME_BIN (PID $(echo ${PID})) 正在运行..."
	else
		echo -e "${Info} $NAME_BIN 未运行 !"
		RETVAL=1
	fi
}
do_restart(){
	do_stop
	sleep 2s
	do_start
}
case "$1" in
	start|stop|restart|status)
	do_$1
	;;
	*)
	echo "使用方法: $0 { start | stop | restart | status }"
	RETVAL=1
	;;
esac
exit $RETVAL
